{
 "extends": [
    "stylelint-rscss/config",
    "stylelint-config-standard"
  ],
  "ignoreFiles": [],
  "rules": {
    "rscss/no-descendant-combinator": false,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "function",
          "if",
          "each",
          "include",
          "mixin"
        ]
      }
    ]
  }
}
